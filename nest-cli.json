{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "mqtt": {
      "type": "application",
      "root": "apps/mqtt",
      "entryFile": "main",
      "sourceRoot": "apps/mqtt/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mqtt/tsconfig.app.json"
      }
    },
    "grpc": {
      "type": "application",
      "root": "apps/grpc",
      "entryFile": "main",
      "sourceRoot": "apps/grpc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/grpc/tsconfig.app.json"
      }
    },
    "graphql": {
      "type": "application",
      "root": "apps/graphql",
      "entryFile": "main",
      "sourceRoot": "apps/graphql/src",
      "compilerOptions": {
        "tsConfigPath": "apps/graphql/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}